{
  "name": "node-server",
  "description": "Node Server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "export NODE_ENV='development' && nodemon ./bin/server.js --watch ./src --watch ./bin",
    "test": "export NODE_ENV='test' && mocha --reporter spec --no-timeouts --exit",
    "start": "export NODE_ENV='production' && node ./bin/server.js"
  },
  "dependencies": {
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-ws": "4.0.0",
    "moment": "2.24.0",
    "moment-timezone": "0.5.23",
    "underscore": "1.9.1",
    "morgan": "1.9.1",
    "helmet": "3.15.1"
  },
  "devDependencies": {
    "nodemon": "1.19.4",
    "mocha": "5.2.0",
    "should": "13.2.3",
    "supertest": "3.4.2"
  }
}
